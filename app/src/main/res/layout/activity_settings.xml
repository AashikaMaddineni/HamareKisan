<?xml version="1.0" encoding="UTF-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#FAFAFA"
    android:layout_height="match_parent"

    >

    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/rubik_medium"
            >


            <TextView
                android:id="@+id/Info"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:drawableStart="@drawable/ic_baseline_person_24"
                android:padding="10dp"
                android:text=" Personal Information"
                android:textColor="#000000"
                android:textSize="15sp"
                android:textStyle="italic" />

            <TextView
                android:id="@+id/edit_profile"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Info"
                android:layout_marginLeft="25dp"
                android:fontFamily="@font/rubik_medium"
                android:padding="10dp"
                android:text=" Edit profile"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold" />


            <TextView
                android:id="@+id/account_settings"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/edit_profile"
                android:layout_marginLeft="25dp"
                android:fontFamily="@font/rubik_medium"
                android:padding="10dp"
                android:text="Account Settings"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/manage_account"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/account_settings"
                android:layout_marginLeft="25dp"
                android:fontFamily="@font/rubik_medium"
                android:padding="10dp"
                android:text="Manage Accounts"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold" />


            <TextView
                android:id="@+id/support"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/manage_account"
                android:layout_marginTop="20dp"
                android:padding="10dp"
                android:text=" Support"
                android:textColor="#000000"
                android:textSize="15sp"
                android:textStyle="italic" />

            <TextView
                android:id="@+id/About"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/support"
                android:layout_marginLeft="25dp"
                android:fontFamily="@font/rubik_medium"
                android:padding="10dp"
                android:text="About"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold"/>

            <TextView
                android:id="@+id/privacypolicy"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/About"
                android:layout_marginLeft="25dp"
                android:fontFamily="@font/rubik_medium"
                android:padding="10dp"
                android:text="Privacy Policy"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold"/>

            <TextView
                android:id="@+id/terms"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/privacypolicy"
                android:layout_marginLeft="25dp"
                android:fontFamily="@font/rubik_medium"
                android:padding="10dp"
                android:text="Terms &amp; Conditions"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold"/>

            <TextView
                android:id="@+id/feedback"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/terms"
                android:layout_marginLeft="25dp"
                android:fontFamily="@font/rubik_medium"
                android:padding="10dp"
                android:text="Feedback"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold"/>


            <TextView
                android:id="@+id/action"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/feedback"
                android:layout_marginTop="20dp"
                android:padding="10dp"
                android:text=" Actions"
                android:textColor="#000000"
                android:textSize="15sp"
                android:textStyle="italic" />

            <TextView
                android:id="@+id/addaccount"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/action"
                android:layout_marginLeft="25dp"
                android:fontFamily="@font/rubik_medium"
                android:padding="10dp"
                android:text="Add Account"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/logout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/addaccount"
                android:layout_marginLeft="25dp"
                android:fontFamily="@font/rubik_medium"
                android:padding="10dp"
                android:text="Logout"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold" />




        </RelativeLayout>


    </androidx.coordinatorlayout.widget.CoordinatorLayout>

</ScrollView>
